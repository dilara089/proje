@bot.event
async def on_ready():
    print(f'{bot.user} olarak giriş yaptık')

@bot.command()
async def hello(ctx):
    await ctx.send(f'Merhaba {bot.user}! Ben bir botum!')


@bot.command()
async def hello(ctx):
    await ctx.send(f'Merhaba {bot.user}! merhaba')

@bot.command()
async def heh(ctx, count_heh = 5):
    await ctx.send("he" * count_heh)
@bot.command()
async def merhaba(ctx, count_heh = 5):
    await ctx.send("he" * count_heh)


